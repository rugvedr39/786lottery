<script>
    let url = window.location.pathname;
    let check = document.querySelector(`.van-tabbar-item[data-url='${url}']`);
    document.querySelector('#nav_checkUrl').setAttribute('data-url', url);
    if (check) check.classList.add('block-click');
    let pathname = document.querySelector('#nav_checkUrl').getAttribute('data-url');
    let listNavs = document.querySelector('#nav_checkUrl .van-hairline--top-bottom').children;
    let select = 0;
    switch (pathname) {
        case '/home':
            select = 0;
            break;
        case '/promotion' :
            select = 1;
            break;
        case '/promotion/myTeam':
            select = 1;
            break;
        case '/promotion/promotionDes':
            select = 1;
            break;
        case '/promotion/tutorial':
            select = 1;
            break;
        case '/reward':
            select = 2;
            break;
        case '/wallet':
            select = 3;
            break;
        case '/mian':
            select = 4;
            break;
        default:
            break;
    }

    // let arr = [
    //     { id: 1, src: '/images/home.png' },
    //     { id: 2, src: '/images/checkIn.png' },
    //     { id: 3, src: '/images/invite.webp' },
    //     { id: 4, src: '/images/wallet2.png' },
    //     { id: 5, src: '/images/me2.png' },
    // ];

    let arr = [
        { id: 1, src: '/img/nav/home_active.svg' },
        { id: 2, src: '/img/nav/gift_active.svg' },
        { id: 3, src: '/images/invite.webp' },
        { id: 4, src: '/images/wallet2.png' },
        { id: 5, src: '/img/nav/members_active.svg' },
    ];

    listNavs[select].children[1].children[0].children[0].setAttribute('src', arr[select].src); // img
    listNavs[select].children[1].children[1].classList.add('action'); // name active
</script>