          alertMess('Copy successful', $(this));
          console.log($(".address").attr('data-clipboard-text'))
        })

        function alertMess(text, sic, time = 1000) {
            $('body').append(
                `
                <div data-v-1dcba851="" class="msg">
                    <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" style=""> ${text} </div>
                </div>
                `
            );
            setTimeout(() => {
                $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                setTimeout(() => {
                    $('.msg').remove();
                }, 100);
                sic.removeClass('block-click');
            }, time);
        }

        $("#txHash-form").submit((e) => {
            e.preventDefault()
            $('.myloading-overlay').fadeIn(200)

            $.ajax({
                type: "POST",
                url: "/api/webapi/confirmUSDTTransaction",
                data: {
                    txHash: $("#txHash").val()
                },
                dataType: "json",
                success: function (response) {
                    $('.myloading-overlay').fadeOut(200)

                    let {message, status} = response

                    if (!status) {
                        alertMess(message, $(this), 4000)
                    } else {
                        alertMess(message, $(this))
                        location.href = "/mian"
                    }
                }
            })
        })

        $.ajax({
            type: "POST",
            url: "/api/webapi/recharge/check",
            data: {
                checlk: "idCheclk",
            },
            dataType: "json",
            success: function (response) {
                let data = response.datas;
                let infoBank = response.infoBank;
                if (!data) return;
                let i = 1;
                if (data.type != "tron")
                    return

                $('.money').text(data.money + " inr");

                $.ajax({
                    type: "GET",
                    url: "/api/webapi/currentUSDT",
                    success: function (resp) {
                        console.log(resp)
                        if (resp.message != "success") {
                            $('#currentUSDT').text("Error fetching current price")
                            return
                        }

                        $('#currentUSDT').text(resp.value)

                        $('#toTransfer').text(`${parseFloat(data.money / resp.value).toFixed(2)} USDT`)
                    }
                })
            }
        });
