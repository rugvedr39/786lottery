    <script>
        $('.pay-box:eq(0) .item').click(function (e) {
            e.preventDefault();
            $('.pay-box:eq(0) .item').removeClass('action');
            $(this).addClass('action');
            $('.pay-box:eq(0) .item').find('.icon').remove();
            $(this).append(`<div data-v-67caa467="" class="icon">
                                <i data-v-67caa467="" class="van-icon van-icon-success" style="color: rgb(255, 255, 255); font-size: 14px;"> </i>
                            </div>`);

            if ($(this).attr("type") == "fiat") {
                $(".addBankCard").fadeIn(0)
                $(".addUSDTAddress").fadeOut(0)
            } else {
                $(".addBankCard").fadeOut(0)
                $(".addUSDTAddress").fadeIn(0)
            }
        });
        function alertMess(text, sic) {
            $('body').append(
                `
                <div data-v-1dcba851="" class="msg">
                    <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" style=""> ${text} </div>
                </div>
                `
            );
            setTimeout(() => {
                $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                setTimeout(() => {
                    $('.msg').remove();
                }, 100);
                sic.removeClass('block-click');
            }, 1000);
        }
        $('button').click(function (e) {
            e.preventDefault();

            let type = $('.pay-box:eq(0) .list').find('.action').attr('type');

            let money = $('input:eq(0)').val().trim();
            let password = $('input:eq(1)').val().trim();
            if (money && password && money >= 300) {
                $.ajax({
                    type: "POST",
                    url: "/api/webapi/withdrawal",
                    data: {
                        money: money,
                        password: password,
                        type
                    },
                    dataType: "json",
                    success: function (response) {
                        $(this).addClass('block-click');
                        if (response.status == true) {
                            $('.money').text("₹" + response.money + '.00');
                        }
                        alertMess(response.message, $(this));
                    }
                });
            } else if (!money || !password) {
                $(this).addClass('block-click');
                alertMess('Please enter complete information', $(this));
            } else {
                $(this).addClass('block-click');
                alertMess('Withdrawal amount range 300-unlimited', $(this));
            }
        });
    </script>
