        $('.payment-method .item').click(function (e) { 
            e.preventDefault();

            $('.payment-method .item').removeClass('action');
            $(this).addClass('action');
            $('.payment-method .item').find('.icon').remove();
            $(this).append(`<div data-v-67caa467="" class="icon">
