        $('#create-bank').click(function (e) { 
            e.preventDefault();
            $(this).addClass('block-click');
            let money = $('input').val().trim();
            let type = $('.payment-method .list').find('.action').attr('type');
            // const validTypes = ["tron", "wepay", "xdpay"]
            const validTypes = ["okpay", "tron" ,"rspay"]
            if(type =="rspay"){
                window.location.href = `/wallet/paynow/manual_upi?am=${money}`
            }

            // check if payment method is selected
            if (!validTypes.includes(type)) {
                alertMess("please select payment method first", $(this));
                return
            }

            if (money && money >= 100 && money <= 10000000000) {
                $.ajax({
                    type: "POST",
                    url: "/api/webapi/recharge",
                    data: {
                        money: money,
                        type: type,
                    },
                    dataType: "json",
                    success: function (response) {
                        let data = response.datas;
                        if (!response.status) {
                            alertMess(response.message, $(this), 5000);
                            $("#create-bank").removeClass('block-click');
                            return
                        }

                        $('.Loading').fadeIn(0);
                        setTimeout(() => {
                            $('.Loading').fadeOut(0);
                            location.reload();
                            // setTimeout(() => {
                            //     location.href = data.url;
                            // }, 100);
                        }, 1500);
                    }
                });
                return true;
            }
            $(this).addClass('block-click');
            alertMess('Deposit amount range : 100~10000000000', $(this));
        });
        $('.cancel').click(function (e) { 
