    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="/js/client.js"></script>
    <script>
        let totalData = null;

        $(".myBtn").on("click", function() {
            $(".myBtn").removeClass("active")
            $(this).addClass("active")

            if (totalData != null) {
                MemRender(`${$(this).data("level")}`)
            }
        })

        $(".ipt").on("keyup", function() {
            var value = $(this).val().toLowerCase();
            $("#van-list .item").filter(function() {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });

        $('.nav .van-tabbar-item:eq(0) img').attr('src', '/images/home1.png');
        $('.nav .van-tabbar-item:eq(0) .name').removeClass('action');
        $('.nav .van-tabbar-item:eq(2) .name').addClass('action');

        function getStaredPhone(phone) {
            return phone.substr(0, 3) + "****" + phone.substr(7, 4)
        } 

        function MemRender(type) {
            if (type == "f1") {
                datas = totalData.f1;
            }  else if (type == "f2") {
                datas = totalData.f2;
            } else if (type == "f3") {
                datas = totalData.f3;
            } else {
                datas = totalData.f4;
            }

            let html = '';
            datas.map((data) => {
                let phone = data.phone;

                if (type != "f1") phone = getStaredPhone(phone)
                html += `
                        <div data-v-0ff6946a="" class="item">
                            <div data-v-0ff6946a="" class="bd van-row">
                                <div data-v-0ff6946a="" class="c-tc van-col van-col--4">${data.id_user}</div>
                                <div data-v-0ff6946a="" class="c-tc van-ellipsis van-col van-col--7">${phone}</div>
                                <div data-v-0ff6946a="" class="c-tc red van-col van-col--6">${data.totalRecharge}</div>
                                <div data-v-0ff6946a="" class="c-tc van-col van-col--7">
                                    <span data-v-0ff6946a="">${parseFloat(data.totalBet).toFixed(2)}</span>
                                </div>
                            </div>
                            <div data-v-0ff6946a="" class="bdshow" style="display: none;">
                                <div data-v-0ff6946a="" class="c-row c-row-between">
                                    <div data-v-0ff6946a="">Personal revenue:0</div>
                                    <div data-v-0ff6946a="">Personal Recharge:0</div>
                                </div>
                                <div data-v-0ff6946a="">Registration time:2021-06-02 14:40:23</div>
                                <div data-v-0ff6946a="">Junior member:0</div>
                            </div>
                        </div>
                        `;
            });
            $('#van-list').html(html);
        }
        $.ajax({
            type: "GET",
            url: "/api/webapi/myTeam",
            dataType: "json",
            success: function (response) {
                console.log(response)
                $('.Loading').fadeOut(0);
                $('#total-mem').text(`Direct Team(${response.f1.length} People)`);
                totalData = response;
                MemRender("f1")
            }
        });
    </script>
