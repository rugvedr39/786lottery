        html,
        body {
            height: 100%;
            width: 100%;
            background-color: #9195a3;
            padding: 0;
            margin: 0;
        }
        .vipLevel-container {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            gap: 10px;
            font-weight: bold;
        }
        .vipLevel-data {
            font-size: large;
        }
        .num {
            color: #50a3ff;
            font-weight: bold;
        }
        .stats_box {
            width: 100%;
            display: flex;
            justify-content: space-between;
            flex-wrap: wrap;
            row-gap: 10px;

            .stats_box_item {
                width: 49%;
                border-radius: 4px;
                height: 78px;
                display: flex;
                flex-direction: column;
                align-items: center;
                justify-content: center;
                padding: 10px 0;

                &:nth-child(1) {
                    background: #eceffe;
                    width: 100%;
                    height: fit-content;

                    .stats_box_item_title {
                        color: #506eed;
                    }
                }
                &:nth-child(2) {
                    background: #f7e8e8;
                    width: 100%;
                    height: fit-content;

                    .stats_box_item_title {
                        color: #64bc73;
                    }
                }
                &:nth-child(3) {
                    background: #eff9f0;

                    .stats_box_item_title {
                        color: #64bc73;
                    }
                }
                &:nth-child(4) {
                    background: #e6f7f6;

                    .stats_box_item_title {
                        color: #64bc73;
                    }
                }
                &:nth-child(5) {
                    background: #fff7e5;

                    .stats_box_item_title {
                        color: #64bc73;
                    }
                }
                &:nth-child(6) {
                    background: #f7e8e8;

                    .stats_box_item_title {
                        color: #64bc73;
                    }
                }

                .stats_box_item_title {
                    font-size: 14px;
                    font-weight: 600;
                    color: #77819b;
                    display: flex;
                    align-items: center;
                    gap: 6px;
                }

                .stats_box_item_stats {
                    font-weight: bold;
                    color: #3e424d;
                    font-size: 12px;
                    margin-top: 8px;

                    &.ss {
                        width: 90%;
                        display: flex;
                        align-items: center;
                        justify-content: space-around;
                        margin-top: 15px;

                        & .substats {
                            display: flex;
                            flex-direction: column;
                            align-items: center;
                            gap: 6px;
                        }
                    }
                }
            }
        }
        .sharelink_box {
            margin-top: 20px;
            padding: 10px;
            box-sizing: border-box;
            border-radius: 4px;
            background-image: linear-gradient(180deg,#f3f7fb,#e0e9f1);

            .sharelink_title {
                display: flex;
                justify-content: center;
                align-items: center;
                color: #333;
                gap: 8px;
                font-size: 16px;
                font-weight: 700;
                margin-bottom: 20px;
            }

            .url_title {
                display: flex;
                font-size: 16px;
                color: #666;
            }

            .url_content {
                background: #fff;
                border-radius: 4px;
                display: flex;
                justify-content: center;
                align-items: center;
                margin-top: 8px;
                color: #000;
                font-size: 14px;
                padding: 10px;
                box-sizing: border-box;
            }

            .sharelink_bottom {
                width: 100%;
                display: flex;
                justify-content: space-between;
                margin-top: 16px;
            }

            .sharelink_btn {
                width: 48%;
                box-sizing: border-box;
                height: 42px;
                border-radius: 4px;
                display: flex;
                justify-content: center;
                align-items: center;
                color: #fff;
                font-size: 12px;
                font-weight: 600;
                padding: 0 12px;
                gap: 20px;
                background: #3f9368;

                &:nth-child(2) {
                    background: #506eed;
                }
            }
        }
        .post_box {
            margin-top: 20px;
            width: 100%;
            height: 146px;
            border-radius: 4px;
            padding: 10px 20px;
            box-sizing: border-box;
            background: url(/img/promotion/banner.png) no-repeat 50%;
