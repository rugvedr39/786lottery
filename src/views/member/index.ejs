        html,
        body {
            height: 100%;
            width: 100%;
            background-color: #9195a3;
            padding: 0;
            margin: 0;
        }

        .block-click {
            pointer-events: none;
        }

        .navbar {
            background: #3f9368;
        }

        .infoName .name,
        .infoName .number {
            font-weight: 700;
            font-size: 15px !important;
            color: #fff;
        }

        .infoName .invite_code,
        .infoName .id {
            color: #fff;
            opacity: .8;
            font-size: 13px !important;
        }

        .infoItem .system_wallet_box {
            width: 100%;
            background-size: 100% 100%;
            padding-bottom: 20px;
            position: relative;
            z-index: 2;
        }

        .infoItem .title_box {
            font-family: buttersans-Regular, sans-serif;
            font-weight: 900;
            font-size: 16px;
            color: #004722;
            display: flex;
            align-items: center;
            display: flex;
            justify-content: space-between;
            margin-bottom: 12px;
        }

        .infoItem .title_box_mid {
            display: flex;
            gap: 8px;
        }

        .infoItem .funds_box {
            display: flex;
            align-items: center;
            width: 100%;
            justify-content: space-between;
        }

        .infoItem .funds_box .money {
            font-weight: 500;
            font-size: 15px;
            color: #3e424d;
        }

        .infoItem .funds_box .money .txt {
            font-weight: 700;
            font-size: 16px;
            color: #3e424d;
            margin-left: 5px;
        }

        .infoItem .funds_actbox {
            width: 100%;
            display: flex;
            justify-content: space-around;
            align-items: center;
            padding-bottom: 13px;
        }

        .infoItem .funds_actbox_item {
            flex: 1;
            display: flex;
            flex-direction: column;
            gap: 8px;
            align-items: center;
            font-weight: 500;
            font-size: 12px;
            color: #494949;
        }

        .infoItem .funds_actbox_item img {
            width: 32px;
        }

        .wallet_box {
            display: flex;
            gap: 8px;
            flex-shrink: 1;
            overflow: auto;
            /* position: relative; */
            padding-left: 14px;
            /* justify-content: center; */
        }

        .wallet_box .wallet_box_item {
            display: flex;
            width: 221px;
            height: 94px;
            padding: 0 10px;
            flex-shrink: 0;
            justify-content: center;
            gap: 10px;
            background: url(/img/mine/wallet_bg.png) no-repeat 50%;
            background-size: 100% 100%;
            border-radius: 14px;
            padding-top: 4px;
        }

        .wallet_box .wallet_box_item .wallet_box_item_content {
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 7px;
            width: 100%;
        }

        .wallet_box_item_inner {
            display: flex;
            width: 100%;
            align-items: center;
            gap: 20px;
            justify-content: space-between;
        }

        .wallet_box_item_inner img {
            width: 20px;
        }

        .wallet_box_item_content .funds_actbox_item {
            flex: 1;
            display: flex;
            flex-direction: column;
            gap: 8px;
            align-items: center;
            font-weight: 500;
            font-size: 12px;
            color: #494949;
        }

        .wallet_box .wallet_box_item_title {
            font-weight: 600;
            font-size: 15px;
            color: #004722;
        }

        .wallet_box .wallet_box_item_value {
            font-weight: 700;
            font-size: 16px;
            color: #3e424d;
            margin-top: 10px;
        }

        .otherinfo_box {
            width: calc(100% - 28px);
            margin: 0 auto;
            border-radius: 14px;
            display: flex;
            align-items: center;
            justify-content: space-between;
        }

        .otherinfo_box .otherinfo_item {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            width: 23%;
            gap: 7px;
            height: 70px;
            font-size: 12px;
            color: #494949;
            background: #fff;
            border-radius: 8px;
            position: relative;
        }

        .otherinfo_box .otherinfo_item img {
            width: 32px;
        }

        .overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, .3);
            z-index: 1000;
        }

        .overlay {
            display: none;
        }

        .modal {
            width: 80%;
            min-height: 200px;
            background-color: #fff;
            padding: 10px;
            border-radius: 10px;
            box-shadow: 0 0 10px rgba(0, 0, 0, .3);
            position: fixed;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
        }

        .modal-close-btn {
            position: absolute;
            align-items: center;
            justify-content: center;
            bottom: -70px;
            left: 50%;
            transform: translateX(-50%);
            font-size: 50px;
        }

        .modal-close-btn i {
            color: #fff;
            font-size: 50px;
        }

        .modal-header {
            text-align: center;
            border-radius: 10px;
            color: #222;
        } 

        .message-modal-content {
            color: #2c2c2c;
            padding: 10px;
        }

        .custom_in {
            padding: 5px 10px;
            border-radius: 10px;
            border: 1px solid #ccc;
        }
    </style>
