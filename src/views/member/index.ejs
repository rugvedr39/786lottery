    <script>
        $(".overlay").fadeOut(0);
        function alertMess(text, sic, dur = 1000) {
            $('body').append(
                `
                <div data-v-1dcba851="" class="msg">
                    <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" style=""> ${text} </div>
                </div>
                `
            );
            setTimeout(() => {
                $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                setTimeout(() => {
                    $('.msg').remove();
                }, 100);
                sic.removeClass('block-click');
            }, dur);
        }
        /** 
         * @param {string} number
        */
        function getHiddenNumber(number) {
            // return number in format 123****789
            return number.replace(/(\d{3})\d{4}(\d{3})/, '$1****$2');
        }
        fetch('/api/webapi/GetUserInfo')
            .then(response => response.json())
            .then(data => {
                if (data.status === false) {
                    unsetCookie();
                    return false;
                };
                $('.infoName .name').text(data.data.name_user);
                $('.infoName .id').text(`ID:${data.data.id_user}`);
                $('.infoName .id').attr('data-clipboard-text', '');
                $('.infoName .number').text(`Account ${getHiddenNumber(data.data.phone_user)}`);
                $('.infoName .invite_code').text(`Invitation code ${data.data.code}`);
                $('.infoItem .money .txt').text(`${parseFloat(data.data.money_user).toFixed(2)}`);
            });

        fetch("/api/webapi/jili/userInfo")
            .then(response => response.json())
            .then(data => {
                if (data.status === false) {
                    // unsetCookie();
                    $('#jili-balance-box').text("unable to fetch");
                    return;
                };

                $('#jili-balance-box').text(` ₹ ${parseFloat(data.message.Data[0].Balance).toFixed(2)} `);
            })

        fetch("/api/webapi/kingmaker/balance")
            .then(response => response.json())
            .then(data => {
                if (data.status === false) {
                    $('#kingmaker-balance-box').text("unable to fetch");
                    return;
                };

                $('#kingmaker-balance-box').text(` ₹ ${parseFloat(data.message.bal).toFixed(2)} `);
