        $("#jili-modal-deposit-btn").click(function () {
            const amount = $("#deposit-amount").val();

            if (amount == "" || amount < 1) {
                alertMess("please enter valid amount", $(this));
                return;
            }

            $('.Loading').fadeIn(10);
            fetch("/api/webapi/jili/deposit", {
                method: "POST",
                headers: {
                    "Content-Type": "application/json"
                },
                body: JSON.stringify({
                    amount
                })
            })
                .then(response => response.json())
                .then(data => {
                    $('.Loading').fadeOut(10);
                    if (data.status == false && data.message == "Insufficient balance") {
                        alertMess("Insufficient balance", $(this));
                        return;
                    } else if (data.status == false) {
                        if (data.message == "Please recharge at least 1000") {
                            alertMess("Please recharge at least 1000", $(this));
                            return;
                        }
                        alertMess("unable to deposit at the moment", $(this));
                        console.error("[ERROR]:", data);
                        return;
                    };

                    $('#jili-balance-box').text(` ₹ ${parseFloat(data.data.CoinAfter).toFixed(2)} `);
